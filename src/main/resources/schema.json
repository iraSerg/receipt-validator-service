{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "user",
    "createdAt",
    "items"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "user": {
      "type": "string",
      "format": "uuid"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "name",
          "price",
          "quantity",
          "sum",
          "invoiceType",
          "invoiceSum",
          "productType",
          "paymentType"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "price": {
            "type": "integer"
          },
          "quantity": {
            "type": "integer"
          },
          "sum": {
            "type": "integer"
          },
          "invoiceType": {
            "type": "integer"
          },
          "invoiceSum": {
            "type": "integer"
          },
          "productType": {
            "type": "integer"
          },
          "paymentType": {
            "type": "integer"
          }
        }
      }
    }
  }
}
